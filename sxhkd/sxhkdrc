
super + n
 	~/.local/bin/network.sh {up, down}

super + v
	$TERMINAL -e vim-x11

super + d
	dmenu_run

super + w
	$BROWSER

super + {equal, minus, BackSpace}
 	~/.local/bin/volume.sh {up, down, toggle}

super + shift + BackSpace
	~/.local/bin/exit.sh 

super + f
	$TERMINAL -e lf

super + o
  zathura

Print 
	maim -u ~/Pictures/screen/$(date +%s).png

ctrl + Print 
	~/.local/bin/image.sh

super + Print
	~/.local/bin/screencast.sh

super + p 
	mpv --ytdl-format="bestvideo[height<=420]+bestaudio/best[height<=420]" $(xclip -o)
